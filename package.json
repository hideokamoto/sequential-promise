{
  "name": "sequential-promise",
  "version": "0.1.0",
  "description": "simply async function runner as a sequential",
  "main": "dist/index.js",
  "scripts": {
    "build": "rollup -c",
    "tsc": "tsc",
    "test": "jest",
    "lint": "eslint 'libs/*.{ts}'",
    "doc": "typedoc --out docs libs/ && touch docs/.nojekyll",
    "postversion": "git push origin $(git describe --abbrev=0 --tags)",
    "test:watch": "jest --watch",
    "test:dev": "jest --watch --silent=false --verbose false --coverage"
  },
  "keywords": [
    "stripe"
  ],
  "bugs": {
    "url": "https://github.com/hideokamoto/sequential-promise/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hideokamoto/sequential-promise.git"
  },
  "author": "Hidetaka Okamoto <info@wp-kyoto.net> (https://wp-kyoto.net)",
  "license": "MIT",
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/__tests__/*/*.+(ts|tsx|js)"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "git add"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.3",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "@typescript-eslint/typescript-estree": "^2.0.0",
    "eslint": "^6.2.2",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^9.2.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^3.0.4",
    "jest": "^24.9.0",
    "lint-staged": "^9.2.5",
    "rollup": "^1.20.3",
    "rollup-plugin-typescript2": "^0.24.0",
    "ts-jest": "^24.0.2",
    "tslib": "^1.10.0",
    "typedoc": "^0.15.0",
    "typescript": "^3.6.2"
  }
}
